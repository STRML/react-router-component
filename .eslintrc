{
  root: true,
  parser: "babel-eslint",
  extends: "eslint:recommended",
  rules: {
    "strict": 0,
    "quotes": 0,
    "no-use-before-define": [1, "nofunc"],
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "camelcase": 0,
    "dot-notation": 0,
    "no-multi-spaces": 0
  },
  env: {
    browser: true,
    node: true,
    mocha: true,
    es6: true
  }
}
